{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": ["error", "always", {"ignorePackages": true}],
        "no-eval": "warn",
        "consistent-return": "warn"
    }
}
